<div class="container row-space-5">
  <div class="row">
    <div class="col-md-12">
      <div class="panel panel-default">
        <div class="panel-heading">今週の収入</div>
        <div class="panel-body">
          <div class="container text-center">
            <% if @reservations.blank? %>
                <h3>現在、表示できる収入はありません</h3><br/>
                <%= link_to "ホストになって収入を得る", new_plan_path, class: "btn btn-normal" %>
            <% else %>
                <%= column_chart @this_week_revenue, colors: ["#00B3C4"] %>
            <% end %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
